# Flow Terminal — Environment Variables
# Copy this to .env and fill in your credentials

# LSEG App Key (free — generate at workspace.refinitiv.com → App Key Generator)
EIKON_APP_KEY=your_app_key_here

# Local bridge WebSocket port (default: 8765)
FLOW_WS_PORT=8765

# Server mode: host/port for FastAPI relay
FLOW_HOST=0.0.0.0
FLOW_PORT=8000
